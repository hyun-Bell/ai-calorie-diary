  "scripts": {
    "generate": "npx prisma generate",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "pnpm run generate && ts-node -r tsconfig-paths/register src/main.ts nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "prisma migrate deploy && jest --verbose --config ./test/jest-e2e.json --runInBand",
    "test:e2e:all": "prisma migrate deploy && RUN_REAL_API_TESTS=true jest --verbose --config ./test/jest-e2e.json --runInBand"
  },
